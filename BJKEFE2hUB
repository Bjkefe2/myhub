ClickTP.Name = "Click TP"
ClickTP.Parent = PlayerScript
ClickTP.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
ClickTP.BorderColor3 = Color3.fromRGB(162, 163, 165)
ClickTP.BorderSizePixel = 0
ClickTP.Position = UDim2.new(-0.00499999523, 0, 0.375195503, 0)
ClickTP.Size = UDim2.new(0, 200, 0, 19)
ClickTP.Font = Enum.Font.SourceSans
ClickTP.Text = "TP[E]"
ClickTP.TextColor3 = Color3.fromRGB(255, 255, 0)
ClickTP.TextSize = 14.000
ClickTP.MouseButton1Down:Connect(function()

	plr = game.Players.LocalPlayer 
	hum = plr.Character.HumanoidRootPart
	mouse = plr:GetMouse() 

	mouse.KeyDown:connect(function(key)
		if key:lower() == "e" then
			if mouse.Target then 
				hum.CFrame = CFrame.new(mouse.Hit.x, mouse.Hit.y + 5, mouse.Hit.z) 
			end 
		end
	end)

end)


ClickDestroy.Name = "ClickDestroy"
