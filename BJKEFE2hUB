AllValues.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
AllValues.BorderColor3 = Color3.fromRGB(162, 163, 165)
AllValues.BorderSizePixel = 0
AllValues.Position = UDim2.new(-0.00499999523, 0, 0.652823269, 0)
AllValues.Size = UDim2.new(0, 200, 0, 19)
AllValues.Font = Enum.Font.SourceSans
AllValues.Text = "All Values max"
AllValues.TextColor3 = Color3.fromRGB(255, 255, 0)
AllValues.TextSize = 14.000
AllValues.MouseButton1Down:Connect(function()
	for i,v in pairs(game.Players.LocalPlayer:GetDescendants()) do
		if v.ClassName == "NumberValue" or v.ClassName == "StringValue" then
			v.Value = 99999999

			if v.ClassName == "BoolValue" then
				v.Value = true
				for i2,v2 in pairs(game.Workspace[game.Players.LocalPlayer.Name]:GetDescendants()) do
					if v2.ClassName == "NumberValue" or v2.ClassName == "StringValue" then
						v2.Value = 9999999
						if v2.ClassName == "BoolValue" then
							v2.Value = true
							for i3,v3 in pairs(game.ReplicatedStorage:GetDescendants()) do
								if v3.ClassName == "NumberValue" or v3.ClassName == "StringValue" then
									v3.Value = 99999999
									if v3.ClassName == "BoolValue" then
										v3.Value = true

									end
								end
							end

						end
					end
				end
			end
		end
	end
end)
