ClickDestroy.TextSize = 14.000
ClickDestroy.MouseButton1Down:Connect(function()
	plr = game.Players.LocalPlayer 
	mouse = plr:GetMouse() 

	mouse.KeyDown:connect(function(key)
		if key:lower() == "p" then
			if mouse.Target then 
				mouse.Target:Destroy()
			end 
		end
	end)


end)

KickBypassnotallGames.Name = "Kick Bypass(not all Games)"
KickBypassnotallGames.Parent = PlayerScript
KickBypassnotallGames.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
KickBypassnotallGames.BorderColor3 = Color3.fromRGB(162, 163, 165)
KickBypassnotallGames.BorderSizePixel = 0
KickBypassnotallGames.Position = UDim2.new(-0.00499999523, 0, 0.5602808, 0)
KickBypassnotallGames.Size = UDim2.new(0, 200, 0, 19)
KickBypassnotallGames.Font = Enum.Font.SourceSans
KickBypassnotallGames.Text = "Kick Bypass(not All games)"
KickBypassnotallGames.TextColor3 = Color3.fromRGB(255, 255, 0)
KickBypassnotallGames.TextSize = 14.000
KickBypassnotallGames.MouseButton1Down:Connect(function()
	if is_synapse_function then
		local mt = getrawmetatable(game)
		setreadonly(mt,false)
		oldindex = mt.__namecall
		----------------------------------
		mt.__namecall = newcclosure(function(self,...)
			local args = {...}
			local method = getnamecallmethod()
			----------------------------------
			if method == "Kick" or method == "kick" then
				return nil
			end
			return oldindex(self,...)
		end)
	else
		if is_protosmasher_closure  then
			local mt = getrawmetatable(game)
			local oldmt = mt.__index
			make_writeable(mt)

			mt.__index = function(t,i)
				if i == "Kick" then
					return nil
				end
				return oldmt(t,i)
			end


		end

	end
end)
