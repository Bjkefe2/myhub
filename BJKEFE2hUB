for i,v in pairs(game:GetService("CoreGui"):GetDescendants()) do
    if v.Name == "Gui Hub" or v.Name == "Login" or v.Name == "Main Scripts" then
        v:Destroy()
    end
end


function LootGround()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Bjkefe2/LOOTGROUND/main/loot%20battleground"))()
    end



