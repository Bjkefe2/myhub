	for i,v in pairs(game:GetDescendants()) do
		if v.Name == "HumanoidRootPart" and string.find(v.Parent.Name:lower(),TpPlayerBox.Text) then
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame

		end
	end

end)
